@page
@model StoreWebCore.Pages.Shop.TempDataModel
@{
}

    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title></title>
    </head>
    <body>
        <form  method="post" asp-controller="demo" asp-action="save">
            <fieldset>
                <legend>Single values</legend>
                <table>
                    <tr>
                        <td>Age</td>
                        <td><input type="text" name="age" /></td>
                    </tr>
                    <tr>
                        <td>Username</td>
                        <td><input type="text" name="Username" /></td>
                    </tr>

                </table>
            </fieldset>
            <fieldset>
                <legend>Product Information</legend>
                <table>
                    <tr>
                        <td>ID</td>
                        <td><input asp-for="product.Id" /></td>
                    </tr>
                    <tr>
                        <td>Name</td>
                        <td><input asp-for="product.Name" /></td>
                    </tr>
                    <tr>
                        <td>Price</td>
                        <td><input asp-for="product.Price" /></td>
                    </tr>
                   
                </table>
                <input type="submit" value="Save"/>
            </fieldset>

        </form>
    </body>
    </html>